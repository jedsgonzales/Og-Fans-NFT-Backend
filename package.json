{
  "name": "celeb-nft-api",
  "version": "0.0.1",
  "private": true,
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/main.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "webpack": "webpack --stats-error-details --mode development",
    "build-ts": "npm run clean && tsc --noEmit false",
    "build": "cat tsconfig.webpack.json > tsconfig.json && tsc && webpack --stats-error-details --mode development && cat tsconfig.normal.json > tsconfig.json && npm run clean && tsc --noEmit false",
    "api:run-dev": "cross-env NODE_ENV=development ts-node src/services/index | bunyan",
    "api:run": "NODE_ENV=production node ./dist/services/index.js | bunyan",
    "start": "cross-env NODE_ENV=development nodemon --ext js,jsx,json --watch dist  --exec \"npm run api:run-dev\"",
    "contract:listener": "ts-node scripts/ogfans-listener.ts | bunyan",
    "contract:listener-dev": "cross-env NODE_ENV=development ts-node scripts/ogfans-listener.ts | bunyan",
    "test": "cross-env NODE_ENV=development npm run build && cross-env NODE_ENV=development ts-mocha --exit test/*/** | bunyan"
  },
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@edenholdings/web3-libs": "file:../eden-web3-libraries",
    "@metamask/eth-sig-util": "^4.0.0",
    "@truffle/hdwallet-provider": "^2.0.3",
    "apollo-server-express": "^3.6.6",
    "aws-sdk": "^2.1099.0",
    "bip39": "^3.0.4",
    "body-parser": "^1.19.2",
    "bunyan": "^1.8.15",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ethers": "^5.5.4",
    "express": "~4.17.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.17.0",
    "ipfs-http-client": "^56.0.2",
    "luxon": "^2.3.1",
    "merkletreejs": "^0.2.31",
    "node-fetch": "^3.2.1",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-strategy": "^1.0.0",
    "rate-limiter-flexible": "^2.3.7",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "web3": "1.7.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/bunyan": "^1.8.8",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/luxon": "^2.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/passport": "^1.0.7",
    "@types/passport-strategy": "^0.2.35",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^9.2.2",
    "shx": "^0.3.4",
    "ts-loader": "^9.2.5",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.2",
    "webpack": "^5.51.2",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "nodemonConfig": {
    "delay": 2500
  }
}
